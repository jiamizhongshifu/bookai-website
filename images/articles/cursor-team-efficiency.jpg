<?xml version="1.0" encoding="UTF-8"?>
<svg width="800" height="450" viewBox="0 0 800 450" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- 背景 -->
  <rect width="800" height="450" fill="#1A1A2E"/>
  
  <!-- 装饰元素 -->
  <circle cx="120" cy="120" r="60" fill="#16213E" stroke="#4CC9F0" stroke-width="2"/>
  <circle cx="680" cy="330" r="80" fill="#16213E" stroke="#4CC9F0" stroke-width="2"/>
  
  <!-- 团队协作图示 -->
  <g transform="translate(250, 150)">
    <!-- 中心节点 - Cursor -->
    <circle cx="150" cy="75" r="40" fill="#FF6B6B"/>
    <text x="150" y="80" font-family="Arial, sans-serif" font-size="16" font-weight="bold" fill="white" text-anchor="middle">Cursor</text>
    
    <!-- 团队成员节点 -->
    <circle cx="50" cy="25" r="25" fill="#4CC9F0"/>
    <text x="50" y="30" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle">开发者1</text>
    
    <circle cx="50" cy="125" r="25" fill="#4CC9F0"/>
    <text x="50" y="130" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle">开发者2</text>
    
    <circle cx="250" cy="25" r="25" fill="#4CC9F0"/>
    <text x="250" y="30" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle">开发者3</text>
    
    <circle cx="250" cy="125" r="25" fill="#4CC9F0"/>
    <text x="250" y="130" font-family="Arial, sans-serif" font-size="12" fill="white" text-anchor="middle">开发者4</text>
    
    <!-- 连接线 -->
    <line x1="75" y1="25" x2="110" y2="50" stroke="#4CC9F0" stroke-width="2"/>
    <line x1="75" y1="125" x2="110" y2="100" stroke="#4CC9F0" stroke-width="2"/>
    <line x1="225" y1="25" x2="190" y2="50" stroke="#4CC9F0" stroke-width="2"/>
    <line x1="225" y1="125" x2="190" y2="100" stroke="#4CC9F0" stroke-width="2"/>
    
    <!-- 效率提升指示器 -->
    <g transform="translate(100, 175)">
      <rect width="100" height="20" rx="5" fill="#16213E"/>
      <rect width="50" height="20" rx="5" fill="#4CC9F0"/>
      <text x="110" y="15" font-family="Arial, sans-serif" font-size="12" fill="white">效率提升50%</text>
    </g>
  </g>
  
  <!-- 特性列表 -->
  <g transform="translate(300, 350)">
    <circle cx="0" cy="0" r="5" fill="#FF6B6B"/>
    <text x="15" y="5" font-family="Arial, sans-serif" font-size="14" fill="white">共享上下文</text>
    
    <circle cx="120" cy="0" r="5" fill="#FF6B6B"/>
    <text x="135" y="5" font-family="Arial, sans-serif" font-size="14" fill="white">知识库集成</text>
    
    <circle cx="240" cy="0" r="5" fill="#FF6B6B"/>
    <text x="255" y="5" font-family="Arial, sans-serif" font-size="14" fill="white">实时协作</text>
  </g>
  
  <!-- 标题 -->
  <text x="400" y="80" font-family="Arial, sans-serif" font-size="36" font-weight="bold" fill="white" text-anchor="middle">Cursor 团队版实测</text>
  <text x="400" y="120" font-family="Arial, sans-serif" font-size="24" fill="#4CC9F0" text-anchor="middle">如何用AI工具提升开发效率50%</text>
  
  <!-- 装饰线 -->
  <line x1="150" y1="80" x2="650" y2="80" stroke="#FF6B6B" stroke-width="2"/>
  <line x1="200" y1="120" x2="600" y2="120" stroke="#FF6B6B" stroke-width="2"/>
